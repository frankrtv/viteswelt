{
  "name": "sveltekit-starter",
  "version": "1.0.0",
  "type": "module",
  "description": "A modern, opinionated starter template for SvelteKit applications with Tailwind CSS, TypeScript, and dark mode support",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/[your-username]/sveltekit-starter.git"
  },
  "bugs": {
    "url": "https://github.com/[your-username]/sveltekit-starter/issues"
  },
  "homepage": "https://github.com/[your-username]/sveltekit-starter#readme",
  "keywords": [
    "svelte",
    "sveltekit",
    "typescript",
    "tailwindcss",
    "starter",
    "template",
    "dark-mode",
    "responsive"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@sveltejs/kit": "^2.15.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "svelte": "^5.17.3",
    "svelte-check": "^4.1.3",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-svelte": "^2.46.1",
    "postcss": "latest",
    "postcss-load-config": "^6.0.1",
    "prettier": "latest",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte-eslint-parser": "^0.43.0",
    "tailwindcss": "latest",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
}
